{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "images/**",
            "use": "@vercel/static"
        },
        {
            "src": "styles.css",
            "use": "@vercel/static"
        },
        {
            "src": "swiper.esm.js",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}